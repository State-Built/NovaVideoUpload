{
    "name": "state/nova-video-upload",
    "description": "A Laravel Nova field for uploading videos to a video provider.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "ankitpokhrel/tus-php": "^2.1",
        "vimeo/laravel": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "State\\VideoUpload\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "State\\VideoUpload\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
